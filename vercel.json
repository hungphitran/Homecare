{
    "version": 2,
    "functions": {
        "index.js": {
            "runtime": "nodejs18.x"
        }
    },
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node@18"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    },
    "installCommand": "npm install --legacy-peer-deps",
    "buildCommand": "npm run build",
    "outputDirectory": "."
}